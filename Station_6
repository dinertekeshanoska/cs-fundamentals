/*
Enter your query here.
*/
// ------------------------------------- //
SELECT DISTINCT CITY
FROM STATION
WHERE REGEXP_LIKE(CITY, '^[aeiouAEIOU]')
AND REGEXP_LIKE(CITY, '[aeiouAEIOU]$');
// ------------------------------------- //
